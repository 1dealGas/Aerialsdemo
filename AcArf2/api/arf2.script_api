- name: Arf2
  type: table
  members:


  - name: InitArf
    type: function
    parameters:
    - name: buf
      type: table
    - name: is_auto
      type: boolean
    returns:
    - name: before
      type: number
    - name: total_hints
      type: number
    - name: wgo_required
      type: number
    - name: hgo_required
      type: number

  - name: FinalArf
    type: function


  - name: UpdateArf
    type: function
    parameters:
    - name: mstime
      type: number
    - name: table_wgo
      type: table
    - name: table_hgo
      type: table
    - name: table_agol
      type: table
    - name: table_agor
      type: table
    - name: table_wtint
      type: table
    - name: table_htint
      type: table
    - name: table_atint
      type: table
    returns:
    - name: hint_lost
      type: number
    - name: wgo_used
      type: number
    - name: hgo_used
      type: number
    - name: ago_used
      type: number


  - name: SetIDelta
    type: function
    parameters:
    - name: idelta
      type: number

  - name: JudgeArf
    type: function
    parameters:
    - name: table_touch
      type: table
    returns:
    - name: hint_hit
      type: number
    - name: hint_early
      type: number
    - name: hint_late
      type: number
    - name: special_hint_judged
      type: boolean


  - name: SetCam
    type: function
    parameters:
    - name: xscale
      type: number
    - name: yscale
      type: number
    - name: xdelta
      type: number
    - name: ydelta
      type: number
    - name: rotdeg
      type: number

  - name: SetDaymode
    type: function
    parameters:
    - name: is_daymode
      type: boolean

  - name: SetAllowAnmitsu
    type: function
    parameters:
    - name: is_anmitsu_allowed
      type: boolean

  - name: SetJudgeRange
    type: function
    parameters:
    - name: range
      type: number

  - name: SetHintSize
    type: function
    parameters:
    - name: x
      type: number
    - name: y
      type: number
      desc: Optional. y=x if y is not a number